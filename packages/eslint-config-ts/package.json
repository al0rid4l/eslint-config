{
  "name": "@nineteen84/eslint-config-ts",
  "version": "1.0.6",
  "description": "al0ridal's typescript eslint config",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.6.12",
  "type": "module",
  "main": "./index.cjs",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "files": [
    "./index.cjs",
    "./index.mjs"
  ],
  "scripts": {
    "lint": "eslint ./**/*.{cjs,mjs,js} --cache --report-unused-disable-directives",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al0rid4l/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "typescript",
    "config"
  ],
  "author": "al0rid4l",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/al0rid4l/eslint-config/issues"
  },
  "homepage": "https://github.com/al0rid4l/eslint-config#readme",
  "peerDependencies": {
    "eslint": "^8.46.0"
  },
  "dependencies": {
    "@nineteen84/eslint-config-base": "workspace:*",
    "@nineteen84/eslint-config-rules": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0"
  }
}
