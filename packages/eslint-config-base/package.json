{
  "name": "@nineteen84/eslint-config-base",
  "version": "1.0.3",
  "description": "al0ridal's basic eslint config",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.7.6",
  "type": "module",
  "main": "./index.cjs",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "files": [
    "./index.cjs",
    "./index.mjs"
  ],
  "scripts": {
    "lint": "eslint ./**/*.{cjs,mjs,js} --cache --report-unused-disable-directives",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al0rid4l/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "basic",
    "config"
  ],
  "author": "al0rid4l",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/al0rid4l/eslint-config/issues"
  },
  "homepage": "https://github.com/al0rid4l/eslint-config#readme",
  "peerDependencies": {
    "eslint": "^8.46.0"
  },
  "dependencies": {
    "@nineteen84/eslint-config-rules": "workspace:*",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-security-node": "^1.1.1"
  }
}
