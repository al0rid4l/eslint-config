{
  "name": "@nineteen84/eslint-config-vue",
  "version": "1.0.3",
  "description": "al0ridal's browser eslint config",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.7.4",
  "type": "module",
  "main": "index.cjs",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "files": [
    "./index.cjs",
    "./index.mjs"
  ],
  "scripts": {
    "lint": "eslint ./**/*.{cjs,mjs,js} --cache --report-unused-disable-directives",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al0rid4l/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "vue",
    "config"
  ],
  "author": "al0rid4l",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/al0rid4l/eslint-config/issues"
  },
  "homepage": "https://github.com/al0rid4l/eslint-config#readme",
  "peerDependencies": {
    "eslint": "^8.46.0"
  },
  "dependencies": {
    "@nineteen84/eslint-config-browser": "workspace:*",
    "eslint-plugin-vue": "^9.17.0",
    "globals": "^13.21.0"
  }
}
